function welcome (age, ads){
var name = prompt("What is your name?");
alert("Hey " + name + ", Welcome to our site.");
alert("Your age is: " + age);
alert("you live in : " + ads);
var dogyears = age/15;
return dogyears;
}
var dog = welcome (22, "Jamnagar");
alert(dog);
